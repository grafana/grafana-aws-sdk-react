    "@types/jquery": "3.3.38",
    "@types/lodash": "4.14.123",
    "@types/node": "10.14.1",
    "@types/papaparse": "5.2.0",
    "@types/react": "16.8.16",
    "@types/react-test-renderer": "^17.0.1",
    "@types/rollup-plugin-visualizer": "2.6.0",
    "@types/sinon": "^7.5.2",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "pretty-format": "25.1.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
